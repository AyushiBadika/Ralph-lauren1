{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "./parichay/polo/index.html", "use": "@vercel/static" },
    { "src": "/css/style.css", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/static" },
    { "src": "img/*", "use": "@vercel/static" }
  ],
  "headers": [
    {
      "source": "/script.js",
      "headers": [{ "key": "Content-Type", "value": "application/javascript" }]
    },
    {
      "source": "/css/style.css",
      "headers": [{ "key": "Content-Type", "value": "text/css" }]
    },
    {
      "source": "./parichay/polo/css/style.css",
      "headers": [{ "key": "Content-Type", "value": "text/css" }]
    }
  ]
}
