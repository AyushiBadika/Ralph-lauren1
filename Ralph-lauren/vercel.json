{
  "rewrites": [{ "source": "/", "destination": "/mohit/index.html" }],
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "**/*.css",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }]
    },
    {
      "source": "**/*.html",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }]
    }
  ],
  "static": {
    "directory": "./",
    "public": "./public"
  }
}
